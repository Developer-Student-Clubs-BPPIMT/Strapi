    },
    "Description": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "Author": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Read": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Platform": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
